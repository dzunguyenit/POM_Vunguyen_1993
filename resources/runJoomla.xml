<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="tests">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>
	<test name="Joomla testcase on Chrome">
		<parameter name="browser" value="chrome" />
		<parameter name="url"
			value="https://automationfc.joomla.com/administrator/" />
		<parameter name="version" value="2.35" />
		<classes>
			<class name="com.joomla.TC_Joomla" />
		</classes>
	</test>
	<test name="Joomla testcase on Firefox">
		<parameter name="browser" value="firefox" />
		<parameter name="url"
			value="https://automationfc.joomla.com/administrator/" />
		<parameter name="version" value="" />
		<classes>
			<class name="com.joomla.TC_Joomla" />
		</classes>
	</test>
</suite> 
